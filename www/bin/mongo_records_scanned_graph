#!/bin/bash

source "$PWD/../../conf/dash.conf"
source "$PWD/../../conf/mon.conf"
source "$PWD/../../lib/dash_functions.sh"

ex=$(find "$PWD"/../graphs -mmin -$(expr 2 \* $freqdef / 60 ) -name localhost.standalone_MongoDB_mongo_extended.sql3_extended_hostport_`echo "$2" | tr ':' '_'`.scanned.svg)
if [ -z "$ex" ] ; then
"$PWD"/../../graph --database=standalone/MongoDB/mongo_extended.sql3 --table=extended --metric=scanned --where="hostport:$2" --perpixel --width=750 --height=300 --padding=10 --legend=160
fi
echo -e "<div class=\"combinedgraph\">\n<object data=\"/graphs/localhost.standalone_MongoDB_mongo_extended.sql3_extended_hostport_`echo "$2" | tr ':' '_'`.scanned.svg\" type=\"image/svg+xml\" width=\"750\" height=\"300\" style=\"float:left\"></object>\n</div>"

